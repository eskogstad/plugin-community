[
  {
    "command": "community:create",
    "plugin": "@salesforce/plugin-community",
    "flags": [
      "api-version",
      "description",
      "json",
      "loglevel",
      "name",
      "target-org",
      "template-name",
      "url-path-prefix"
    ],
    "alias": ["force:community:create"]
  },
  {
    "command": "community:list:template",
    "plugin": "@salesforce/plugin-community",
    "flags": ["json", "target-org"],
    "alias": ["force:community:template:list", "community:template:list"]
  },
  {
    "command": "community:publish",
    "plugin": "@salesforce/plugin-community",
    "flags": ["api-version", "json", "loglevel", "name", "target-org"],
    "alias": ["force:community:publish"]
  }
]
